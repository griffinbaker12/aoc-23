file_path = 'test.txt'

maps = {}

order = ["ss", "sf", "fw", 'wl', 'lt', 'th', 'hl']

with open(file_path, 'r') as file:
    input = file.read().strip().split('\n\n')
    seeds = {x for x in input[0].split(": ")[1].split(" ")}
    input = input[1:]
    input = [x.split("\n") for x in input]
    print(input, seeds)
    for i, line in enumerate(input):
        ranges = line[1:]
        maps[order[i]] = ranges
    print(maps)
